<Page x:Class="ALabs.SettingsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ALabs"
      mc:Ignorable="d" 
      d:DesignHeight="768" d:DesignWidth="1366"
      Title="SettingsPage">

    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Content="Edit Account" Width="600" Height="100" Click="EditAccount_Click" Margin="383,200,383,5"/>
            <Button Content="Delete Account" Width="600" Height="100" Click="DeleteAccount_Click" Margin="383,0,383,80"/>
            <Button Content="Back" Width="600" Height="100" Click="BackButton_Click" Margin="383,0,383,238"/>
            
        </StackPanel>
    </Grid>
</Page>
