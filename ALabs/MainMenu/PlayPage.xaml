      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ALabs"
      mc:Ignorable="d" 
      d:DesignHeight="768" d:DesignWidth="1366"
      Title="PlayPage">

    <Grid>
        <StackPanel>
            <Button Content="Regex Challenge" Margin="0,200,0,200" Width="600" Height="100" Click="RegexChallenge_Click"/>
            <Button Content="Back" Margin="0,0,0,0" Width="600" Height="100" Click="BackButton_Click"/>
        </StackPanel>
    </Grid>
</Page>
